{
  "name": "@edg.sh/server",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly src/server",
    "start": "npm run migrate && NODE_ENV=production node dist/server",
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.json",
    "eslint": "eslint . --ext ts,tsx",
    "migrate": "dotenv -e prisma/.env -- npx prisma migrate up --experimental",
    "generate:prisma": "npx prisma generate",
    "generate:nexus": "npx ts-node --transpile-only src/schema"
  },
  "dependencies": {
    "@nexus/schema": "0.13.1",
    "@prisma/client": "^2.0.0-beta.8",
    "@prisma/migrate": "^2.1.0",
    "bcryptjs": "2.4.3",
    "bluebird": "^3.7.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express-session": "^1.17.1",
    "graphql": "14.6.0",
    "graphql-shield": "5.7.3",
    "graphql-yoga": "1.18.3",
    "jsonwebtoken": "8.5.1",
    "nexus-prisma": "0.13.0",
    "redis": "^3.0.2",
    "stripe": "^8.56.0",
    "ts-node": "^8.10.2",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@prisma/cli": "^2.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.32",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.6",
    "@types/redis": "^2.8.22",
    "@types/stripe": "^7.13.23",
    "@types/uuid": "^8.0.0",
    "@types/ws": "^7.2.4",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "dotenv-cli": "^3.1.0",
    "eslint": "^7.1.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.9.5"
  }
}
