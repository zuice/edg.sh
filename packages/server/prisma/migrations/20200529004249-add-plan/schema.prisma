// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       String @id @default(uuid())
  name     String
  password String
  email    String @unique
}

model Plan {
  id          String @id @default(uuid())
  name        String
  description String
  price       Int
}
